data <- read.csv("C:/Users/User/OneDrive/Documents/EDA Food Nutrition/Food_Nutrition_Dataset.csv")
View(data)
head(data)
str(data)
summary (data)
colSums(is.na(data))
unique(data$category)
table(data$category)
hist(data$calories,
main = "Distribution of Calories",
xlab = "Calories",
col = "lightblue")
hist(data$calories,
main = "Distribution of Calories",
xlab = "Calories",
col = "pink")
boxplot(data[, c("calories","protein","carbs","fat")],
main = "Boxplot of Nutrients",
col = c("red","green","blue","orange"))
barplot(table(data$category),
main = "Food Categories Count",
xlab = "Category",
ylab = "Count",
las = 2,
col = "purple")
plot(data$calories, data$protein,
main = "Calories vs Protein",
xlab = "Calories",
ylab = "Protein",
col = "blue")
col_vec <- ifelse(data$calories > mean(data$calories), "red", "blue")
plot(data$calories, data$protein,
main = "Calories vs Protein",
xlab = "Calories",
ylab = "Protein",
col = col_vec,
pch = 19)
legend("topright",
legend = c("High Calories", "Low Calories"),
col = c("red", "blue"),
pch = 19)
legend("topright",
legend = levels(colors),
col = 1:length(levels(colors)),
pch = 19)
